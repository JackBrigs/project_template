properties([disableConcurentBuilds()])

pipeline {
	agent {
		label 'master'
	}
	options {
		buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
		timestamps()
	}
	stages{
		stage("Create docker image"){
			steps{
			echo "=============start build==============="
			dir('docker/toolbox') {
				sh 'docker build . '
			}
			}
		}
	}

}